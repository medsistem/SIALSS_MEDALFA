	<parameter name="folcom" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
	P.F_NomPro,
	C.F_OrdCom,
	C.F_FolRemi,
	C.F_ClaDoc,
	C.F_ClaPro,
	M.F_DesPro,
	L.F_ClaLot,
	DATE_FORMAT(F_FecCad, '%d/%m/%Y') AS F_FecCad,
	C.F_Pz,
	C.F_Cajas,
	C.F_Resto,
	C.F_CanCom,
	C.F_Costo,
	C.F_ImpTo,
	C.F_ComTot,
	M.F_PrePro,
	C.F_FolRemi,
	(@csum := @csum + F_ComTot) AS totales,
	C.F_Obser as ObserGNKL,
	I.F_Obser as ObserISEM
FROM
	(SELECT @csum := 0) r,
	tb_compra C,
	tb_lote L,
	tb_medica M,
	tb_proveedor P,
	tb_pedidoisem I
WHERE
	C.F_Lote = L.F_FolLot
AND C.F_ClaPro = M.F_ClaPro
AND C.F_ProVee = P.F_ClaProve
AND C.F_OrdCom = I.F_NoCompra
AND C.F_ClaPro = I.F_Clave
AND C.F_ClaDoc = $P{folcom}
GROUP BY
	L.F_ClaPro, L.F_ClaLot
ORDER BY
	C.F_IdCom;]]>
	</queryString>
